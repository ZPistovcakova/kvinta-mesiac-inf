import tkinter
import random
X_MAX,Y_MAX=800,600
canvas=tkinter.Canvas(width=X_MAX,height=Y_MAX,bg="white")
canvas.pack()

def stvorec(x,y,z):
    canvas.create_rectangle(x-z,y-z,x+z,y+z)

x=random.randint(60,740)
y=random.randint(60,540)
z=50
for i in range(1,6):
    stvorec(x,y,z)
    z=z+5
x=random.randint(60,740)
y=random.randint(60,540)    
for i in range(1,5):
    stvorec(x,y,z)
    z=z+5
